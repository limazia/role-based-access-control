      <div class="card card-chat">
        <div class="card-header">
          <div class="chat-info">
            <div class="room-selected-photo">
              <img src="/assets/images/room.png">
            </div>
            <div class="ms-2">
              <h6 class="room-selected-title"></h6>
              <small class="room-selected-name"></small>
            </div>
          </div>
          <div class="options">
            <i class="fal fa-flag-alt"></i>
            <i class="fal fa-info-circle"></i>
            <%- include("room_permissions.ejs") %>
          </div>
        </div>
        <div class="card-body" id="chat-body">
          <div class="text-center">
            <small class="text-muted">11:00 AM</small>
          </div>
          <div class="answer left">
            <div class="avatar">
              <img src="/assets/images/avatar2.jpg" alt="Bianca">
            </div>
            <div class="name">Bianca Mackenzie</div>
            <div class="text">Teste</div>
            <small class="time">11:38 AM</small>
          </div>
          <div class="answer right">
            <div class="avatar">
              <img src="/assets/images/avatar1.jpg" alt="Acacio">
              <div class="icon-box" data-toggle="tooltip" data-placement="left" title="Administrador">
                <i class="fas fa-user-crown"></i>
              </div>
            </div>
            <div class="text">Teste</div>
            <small class="time">11:38 AM</small>
          </div>
          <div class="answer left">
            <div class="avatar">
              <img src="/assets/images/avatar3.jpg" alt="Julia">
              <div class="icon-box" data-toggle="tooltip" data-placement="left" title="Administrador">
                <i class="fas fa-user-crown"></i>
              </div>
            </div>
            <div class="name">Julia Oliveira</div>
            <div class="text">Teste</div>
            <small class="time">11:40 AM</small>
          </div>
          <div class="messages"></div>
        </div>
        <div class="card-footer">
          <form id="chat">
            <button class="btn btn-plus"><i class="far fa-plus"></i></button>
            <button class="btn btn-send-image"><i class="far fa-images"></i></button>
            <div class="input-group input-message">
              <input type="text" name="message" class="form-control" placeholder="Enviar mensagem para #teste">
              <div class="input-group-append">
                <span class="input-group-text"><i class="fad fa-paper-plane" style="color: #cccccc;"></i></span>
              </div>
            </div>
            <input type="submit" hidden />
          </form>
        </div>
      </div>